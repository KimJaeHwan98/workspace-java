package day02;

import java.util.Scanner;

public class quiz05 {
public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	System.out.print("키를 입력하세요?");
	int b = input.nextInt();
	System.out.print("현재 체중을 입력하세요?");
	int a = input.nextInt();
	
	double d = (b-100) * 0.9;
	double e = (a/d) * 100;
//	System.out.println("표준체중은"+d+ "이고"+"비만도는"+e+ "입니다");
	System.out.printf("표준체중은 %f이고 비만도는 %.1f입니다",d,e);
}
}
