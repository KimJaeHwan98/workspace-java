package day01;

public class EX04 {
	public static void main(String[] args) {
	
		System.out.println("=====================================");
		System.out.println("이름\t나이\t전화번호\t\t회비");
		System.out.println("홍길동\t\""+15+"\"\t"+3672+"-"+1234+"\t\t\\"+20000);
		System.out.println("고길동\ㅅ\""+15+"\"\t"+2238+"-"+1234+"\t\t\\"+30000);
	}
	
}
