package day02;

import java.util.Scanner;

public class quiz05 {
public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	System.out.print("현재 키를 입력하세요?");
	int b = input.nextInt();
	System.out.print("현재 체중을 입력하세요?");
	int a = input.nextInt();

	double c = (b/a-100) * 0.9;
	System.out.println("표준체중은"+b+ "이고"+"비만도는"+c+ "입니다");
 
}
}
