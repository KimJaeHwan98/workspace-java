package for_;

import java.util.Scanner;

public class quiz05 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int a =input.nextInt();
		int b =input.nextInt();
		int sum;
		for(a = 1;a<=9;a++) {
			for(b = 1; b<=9 ;b++) {
			sum = a*b;
			System.out.println(a+"*"+b+"="+sum);
			sum = 0;
			}
		}

	}

}
