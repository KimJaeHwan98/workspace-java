package for_;

public class quiz06 {
public static void main(String[] args) {
	//1부터 시작해 홀수의 합을 구하시오, 
    //단) 합이 10000을 넘지 않는 마지막 홀수값을 구하시오
	int sum = 0,i=1;
	for ( ;sum<10000;i++) {
		if(i%2==0) 
			sum += i;
	}
	i--;
	System.out.println("마지막에 더해진 홀수 :"+i);
	System.out.println("마지막에 더해진 합 :"+sum);
}
}
