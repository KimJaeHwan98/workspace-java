package switch_;

import java.util.Scanner;

public class EX04 {
	public static void main(String[] args) {
		

	Scanner input = new Scanner(System.in);
	String name;
	String a;
	name = "종로3가";
	a = "단성사"
	while(true) {
		System.out.println("1.우리집");
		System.out.println("2.회사등록");
		System.out.println("3.등록보기");
		name = input.next();
		System.out.println("우리집 :"  +name);
		System.out.println("회사 :" + a);
	}
	}
}
