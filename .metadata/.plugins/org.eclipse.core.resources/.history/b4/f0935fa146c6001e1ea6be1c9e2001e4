package for_;

import java.util.Scanner;

public class quiz05 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int sum;
		int a =input.nextInt();
		int b =input.nextInt();
		
		
		for(int i = 1;i<=a;i++) {
			for(int j = 1; j<=b ;j++) {
			sum = a*b;
			System.out.println(i+"*"+j+"="+sum);
			sum = 0;
			}
		}

	}

}
